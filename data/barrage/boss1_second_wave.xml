<?xml version="1.0"?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">
<bulletml type="vertical" xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

	<action label="top">
		<fire>
			<direction type="absolute">180</direction>
			<speed>0</speed>
			<bulletRef label="wave">
				<param>36.4</param>
				<param>144</param>
				<param>20</param>
				<param>10</param>
			</bulletRef>
		</fire>
		<fire>
			<direction type="absolute">180</direction>
			<speed>0</speed>
			<bulletRef label="wave">
				<param>-36.1</param>
				<param>144</param>
				<param>10</param>
				<param>20</param>
			</bulletRef>
		</fire>
	</action>

	<bullet label="wave">
		<action>
			<fire>
				<direction type="absolute">$2</direction>
				<speed>1.1</speed>
				<bulletRef label="dummy" />
			</fire>
			<repeat>
				<times>$4</times>
				<action>
					<repeat>
						<times>10</times>
						<action>
							<fire>
								<direction type="sequence">$1</direction>
								<speed>1.1</speed>
								<bullet />
							</fire>
						</action>
					</repeat>
					<wait>$3</wait>
				</action>
			</repeat>
			<vanish />
		</action>
	</bullet>

	<bullet label="dummy">
		<action>
			<vanish />
		</action>
	</bullet>

</bulletml>